<%= form_with(model: @properti, local: true) do |form| %>
   <% if @properti.errors.any? %>
    <div id="error_explanation">
      <h2><%= @properti.errors.count %>件のエラーがあります。</h2>
      <ul>
      <% @properti.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>
  <div class="properti_name">
    <%= form.label :"物件名" %>
    <%= form.text_field :properties_name %>
  </div>
  <div class="properti_rent">
    <%= form.label :"賃料" %>
    <%= form.text_field :rent %>
  </div>
  <div class="properti_adress">
    <%= form.label :"住所" %>
    <%= form.text_field :adress %>
  </div>
  <div class="properti_age_of_building">
    <%= form.label :"築年数" %>
    <%= form.text_field :age_of_building %>
  </div>
  <div class="properti_note">
    <%= form.label :"備考" %>
    <%= form.text_field :note %>
  </div>
  <%= form.submit %>
<% end %>